{
  "name": "yc/rpc",
  "type": "project",
  "version": "1.0.1",
  "description": "A helper component for hyperf.",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "helper"
  ],
  "support": {
  },
  "suggest": {
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "YC\\RPC\\": "./src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "hyperf": {
      "config": "YC\\RPC\\ConfigProvider"
    }
  },
  "bin": [
  ],
  "scripts": {
    "cs-fix": "php-cs-fixer fix $1",
    "test": "phpunit --colors=always"
  },
  "repositories": [
  ]
}
